{
  "name": "vaporscan",
  "version": "1.0.2",
  "description": "An open-source, client-side SEO auditing tool that detects orphaned pages, broken links, and generates comprehensive site health reports.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "serve out -p 3000",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:run": "vitest --run",
    "test:unit": "vitest --run src",
    "test:unit:watch": "vitest --watch src",
    "test:unit:coverage": "vitest --coverage --run src",
    "test:integration": "vitest --run tests/integration",
    "test:integration:watch": "vitest --watch tests/integration",
    "test:integration:coverage": "vitest --coverage --run tests/integration",
    "test:integration:workflows": "vitest --run tests/integration/workflows",
    "test:integration:state": "vitest --run tests/integration/state",
    "test:integration:storage": "vitest --run tests/integration/storage",
    "test:integration:components": "vitest --run tests/integration/components",
    "test:coverage": "vitest --coverage",
    "test:coverage:all": "vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:critical": "playwright test tests/e2e/specs/critical-paths",
    "test:e2e:accessibility": "playwright test tests/e2e/specs/accessibility",
    "test:e2e:features": "playwright test tests/e2e/specs/features",
    "test:e2e:errors": "playwright test tests/e2e/specs/error-scenarios",
    "test:e2e:cross-browser": "playwright test tests/e2e/specs/cross-browser",
    "test:e2e:report": "playwright show-report",
    "test:a11y": "axe-core src/**/*.tsx",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.28.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "idb": "^8.0.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "papaparse": "^5.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "reactflow": "^11.10.0",
    "recharts": "^2.10.3",
    "sharp": "^0.33.0",
    "tailwind-merge": "^2.2.0",
    "web-vitals": "^3.5.0",
    "zod": "^3.25.76",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.0",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "^16.0.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.6",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fake-indexeddb": "^6.2.5",
    "husky": "^8.0.3",
    "jsdom": "^27.4.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "serve": "^14.2.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  }
}
