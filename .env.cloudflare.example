# Cloudflare Pages Environment Variables
# Copy this file to .env.local for local development
# Set these in Cloudflare Dashboard for production deployment

# Application URL (update with your Cloudflare Pages URL)
NEXT_PUBLIC_APP_URL=https://vaporscan.pages.dev

# GitHub Repository
NEXT_PUBLIC_GITHUB_REPO=https://github.com/sanmak/VaporScan

# Analytics (optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# SEO Advanced Features (optional)
NEXT_PUBLIC_SEO_ADVANCED=false

# Node Environment
NODE_ENV=production

# Cloudflare Pages automatically sets:
# CF_PAGES=1
# CF_PAGES_COMMIT_SHA=<commit-sha>
# CF_PAGES_BRANCH=<branch-name>
# CF_PAGES_URL=<deployment-url>
